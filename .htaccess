
RewriteEngine on

RewriteCond %{HTTP_HOST} ^(www.)?crystalmyth.github.io/lakshya-verma$

RewriteCond %{REQUEST_URI} !^/dist/

RewriteCond %{REQUEST_FILENAME} !-f
